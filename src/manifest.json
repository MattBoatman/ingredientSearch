{
	"name": "Ingredient Search",
	"version": "1.0.0",
	"manifest_version": 2,
	"minimum_chrome_version": "28.0",
	"description": "Search your bookmarks for ingredients",
	"icons": {"16": "icon16.png", "48": "icon48.png", "128": "icon128.png"},
	"background": {"scripts": ["bookmarkSearch.js"], "persistent": false},
	"omnibox": {"keyword" : "boat"},
	"permissions": ["bookmarks"],
	"web_accessible_resources": ["icon48.png"]
}
